<VBox prefHeight="538.0" prefWidth="1203.0" styleClass="white-pane" stylesheets="@styleSheet.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.ListFrontReclamationController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="68.0" prefWidth="1203.0">
         <children>
            <FontAwesomeIconView fill="#fca5a5" glyphName="SEARCH" size="25" />
            <TextField fx:id="txtsearch" prefHeight="44.0" prefWidth="650.0" style="-fx-border-radius: 20px; -fx-border-width: 2px; -fx-background-color: white; -fx-border-color: #fca5a5;">
                     <HBox.margin>
                        <Insets left="20.0" right="170.0" />
                     </HBox.margin>
                  </TextField>
            
            <ComboBox fx:id="txtTri" prefHeight="31.0" prefWidth="260.0" promptText="Trier par :" style="-fx-border-color: #fca5a5;" />
         </children>
      </HBox>
      <TableView fx:id="tableViewReclamation" prefHeight="470.0" prefWidth="1203.0" stylesheets="@styleSheet.css">
        <columns>
          <TableColumn fx:id="txtType" prefWidth="200.0" text="Type" />
          <TableColumn fx:id="txtDes" prefWidth="215.0" text="Description" />
            <TableColumn fx:id="txtEmail" prefWidth="195.0" text="Email" />
            <TableColumn fx:id="txtDate" prefWidth="195.0" text="Date Reclamation" />
            <TableColumn fx:id="txtEtat" prefWidth="195.0" text="Etat" />
            <TableColumn fx:id="txtAction" prefWidth="202.0" text="Action" />
        </columns>
      </TableView>
   </children>
</VBox>
